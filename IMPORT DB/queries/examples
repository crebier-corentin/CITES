SELECT * FROM MAINTABLE WHERE MAINTABLE.wikidataid IN ( SELECT CITES_DISTRIBUTION.wikidataid FROM CITES_DISTRIBUTION INNER JOIN COUNTRIES ON CITES_DISTRIBUTION.countryid = COUNTRIES.ID And countryname = 'SENEGAL' )

SELECT *
FROM MAINTABLE, CITES_DISTRIBUTION, COUNTRIES
WHERE MAINTABLE.wikidataid = CITES_DISTRIBUTION.wikidataid
  AND CITES_DISTRIBUTION.countryid = COUNTRIES.ID
  AND COUNTRIES.countryname = 'Senegal'
  AND CITES_DISTRIBUTION.countrystatus = 'NATIVE'